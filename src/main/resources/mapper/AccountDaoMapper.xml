<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wt.courseselectionsystem.dao.AccountDao">

    <resultMap id="AccountMap" type="com.wt.courseselectionsystem.model.dao.basebean.Account">
        <id column="id" property="id" jdbcType="VARCHAR"/>
        <result column=""/>
    </resultMap>

    <select id="selectById" resultMap="AccountMap">
        select *
        from account
        where id = #{id}
    </select>
</mapper>
